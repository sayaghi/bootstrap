  <script type="text/javascript">
 $(document).ready(function(){
 $('#main').html("<center> <img border='0' src='images/loading.gif' style='margin-top:10%; margin-bottom:10%;'/> </center>");
 $('#main').load('status.php');
 });
 function menu(pg){
 document.getElementById("mybut").click();
 $('#main').html("<center> <img border='0' src='images/loading.gif' style='margin-top:10%; margin-bottom:10%;'/> </center>");
 $('#main').load(pg+'.php');
 }
</script> 
  <!--pie-chart---> 
  <script type="text/javascript">

        $(document).ready(function () {
            $('#demo-pie-1').pieChart({
                barColor: '#3bb2d0',
                trackColor: '#eee',
                lineCap: 'round',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });

            $('#demo-pie-2').pieChart({
                barColor: '#fbb03b',
                trackColor: '#eee',
                lineCap: 'butt',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });

            $('#demo-pie-3').pieChart({
                barColor: '#ed6498',
                trackColor: '#eee',
                lineCap: 'square',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });

           
        });

    </script> 
